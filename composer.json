{
    "name": "jiny/auth-emoney",
    "description": "Authentication E-money and Point system for JinyPHP",
    "keywords": [
        "jinyPHP",
        "framework",
        "auth",
        "emoney",
        "point"
    ],
    "homepage": "http://www.jinyphp.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "hojinlee",
            "email": "infohojin@gmail.com",
            "homepage": "http://www.jinyphp.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Jiny\\AuthEmoney\\": "src/",
            "Jiny\\AuthEmoney\\Http\\Controllers\\": "Http/Controllers/"
        },
        "files" : [
            "Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jiny\\AuthEmoney\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiny\\AuthEmoney\\JinyAuthEmoneyServiceProvider"
            ]
        }
    }
}
